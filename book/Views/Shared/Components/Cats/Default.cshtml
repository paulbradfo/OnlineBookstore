@{
}

@model IEnumerable<string>

<a asp-controller="Home" asp-action="Index" asp-route-bookCat="" class="btn btn-outline-secondary m-1">Home</a>
<br />

@foreach (string t in Model)
{
    <a asp-controller="Home" asp-action="Index" asp-route-bookCat="@t" class="btn btn-outline-secondary m-1">@t</a>
    <br />
}